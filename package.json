    "zod": "^3.22.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/color": "^4.2.0",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
