    "@types/react-window": "^1.8.8",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color": "^5.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.0",
    "framer-motion": "^11.18.2",
    "jotai": "^2.12.5",
    "lexical": "^0.35.0",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.6",
    "next": "14.2.3",
    "radix-ui": "^1.4.2",
